      <category term="服务搭建" scheme="http://sfdotz.me/tags/%E6%9C%8D%E5%8A%A1%E6%90%AD%E5%BB%BA/"/>
    
      <category term="linux" scheme="http://sfdotz.me/tags/linux/"/>
    
